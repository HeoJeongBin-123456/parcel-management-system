{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "nodejs18.x",
      "regions": [
        "icn1"
      ],
      "includeFiles": [
        "public/**/*",
        "data/**/*"
      ]
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}